workgroup = ADBANKING
server string = ADbanking Server
security = share

[backup]
    comment = Sauvegardes d'ADbanking
    path = /var/lib/adbanking/backup
    read only = yes
    public = yes

[ferlo]
    comment = Repertoire d'echange entre Ferlo et ADbanking
    path = /var/lib/adbanking/ferlo
    writable = yes
    public = yes
    create mask = 0777
